@model List<Aktien>
@{
    ViewData["Title"] = "Home Page";
}

<h1>Das sind meine Aktien</h1>


<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Aktienkurs</th>
            <th>Aktienname</th>
            <th>Kurzzeichen</th>
        </tr>
        </thead>
        <tbody>
        
        @{
      
                @foreach (var aktien in Model)
                {
    
                    <tr>
                        <td>@aktien.aktienid</td>
                        <td>@aktien.aktienkurs</td>
                        <td>@aktien.aktienname</td>
                        <td>@aktien.kurzzeichen</td>
                    </tr>
                }
                }
        
        
        </tbody>
    
    
</table>